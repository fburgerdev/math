{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Test) Launch",
            "type": "cppdbg",
            "request": "launch",
            "cwd": ".",
            "program": "${workspaceFolder}/bin/debug_test/test",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true,
                }
            ]
        }
    ]
}